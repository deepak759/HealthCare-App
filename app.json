{
  "expo": {
    "name": "Restauant Menu",
    "slug": "Restauant Menu",
    "version": "1.0.0",
    "orientation": "default",
    
   
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "black"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#212121"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
    
  }
}
